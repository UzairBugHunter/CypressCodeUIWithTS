{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": "./",
      "paths": {
        "@/*": ["src/*"]
      }
    },
    "include": [
      "cypress/**/*.ts",
      "cypress/**/*.tsx"
    ],
    "exclude": [
      "node_modules"
    ]
  }
  